{
    "version": 2,
    "builds": [
      {
        "src": "model_backend/controller.py",
        "use": "@vercel/python"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/model_backend/controller.py" },
      { "src": "/(.*)", "dest": "/src/$1" }
    ]
  }
